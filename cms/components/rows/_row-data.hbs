{{#rowData}}
  <h2>{{title}}</h2>
  <small class="form-text text-muted">{{subtitle}}</small>
{{/rowData}}
<br>
<div class="form-group row" id="field_{{name}}">
  <label class="col-sm-3 control-label">
    {{uiLabel}}
  </label>
  <div class="col-sm-9" id="content_{{name}}">
    {{> (lookup . 'partial') this}}
  </div>
</div>
